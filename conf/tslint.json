{
    "rules": {
        "bitwise": true,
        "classname": true,
        "curly": true,
        "debug": false,
        "dupkey": true,
        "eofline": true,
        "eqeqeq": true,
        "evil": true,
        "forin": false,
        "labelpos": true,
        "label-undefined": true,
        "noarg": true,
        "noconsole": [false, "debug", "info", "time", "timeEnd", "trace"],
        "noconstruct": true,
        "nounreachable": false,
        "noempty": false,
        "oneline": [true, "check-open-brace", "check-catch", "check-else", "check-whitespace"],
        "quotemark": [true, "double"],
        "radix": false,
        "semicolon": true,
        "sub": true,
        "trailing": true,
        "varname": false,
        "whitespace": [false, "check-branch", "check-decl", "check-operator", "check-separator", "check-type"]
    }
}